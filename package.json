{
  "name": "reacttypescriptboilerplate",
  "version": "1.0.0",
  "description": "A Simple boilerplate for reactjs and redux app using typescript",
  "main": "server.js",
  "scripts": {
    "test": "echo \"test\"",
    "postinstall": "npm run build:dll",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "prebuild": "npm run build:clean",
    "build:dll": "node ./config/scripts/dependencies.js",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=production node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "deploy": "pm2 deploy ecosystem.config.js production --force",
    "pre-build": "npm run build  && sudo pm2 startOrRestart ecosystem.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaku14n/reactTypescriptBoilerplate.git"
  },
  "keywords": [
    "reactjs",
    "redux",
    "typescript",
    "boilerplate"
  ],
  "author": "aaku14n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aaku14n/reactTypescriptBoilerplate/issues"
  },
  "moduleDirectories": [
    "node_modules",
    "<rootDir>/src"
  ],
  "homepage": "https://github.com/aaku14n/reactTypescriptBoilerplate#readme",
  "dependencies": {
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "event-source-polyfill": "^0.0.12",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "npmlog": "^4.1.2",
    "offline-plugin": "^5.0.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-intersection-observer": "^6.1.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.22.1",
    "transform-runtime": "^0.0.0",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-pwa-manifest": "^3.7.1"
  }
}
